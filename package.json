{
  "name": "brain-rot-player",
  "displayName": "brain-rot-player",
  "description": "Play brainrot videos while you code",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "repository": "https://github.com/AlexKiippa/brain-rot-player",
  "publisher": "AlexKiippa",
  "icon": "image.jpg",
  "contributes": {
    "commands": [
      {
        "command": "brain-rot-player.brainrot",
        "title": "Brainrot"
      }
    ]
  },
  "pricing": "Free",
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run copy:assets",
    "compile": "tsc -p ./",
    "copy:assets": "node -e \"const fs=require('fs');fs.mkdirSync('out',{recursive:true});fs.copyFileSync('src/player.html','out/player.html');\"",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
